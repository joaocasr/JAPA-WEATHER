<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="313.0" prefWidth="504.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Text fx:id="title">
      <font>
         <Font name="DejaVu Serif Condensed Bold" size="22.0" />
      </font>
    </Text>
    <Text fx:id="geolocalizacao">
        <font>
            <Font name="DejaVu Serif Condensed Bold" size="15.0" />
        </font>
    </Text>
   <TableView fx:id="tinfo" prefHeight="400.0" prefWidth="200.0">
     <columns>
         <TableColumn fx:id="tempmin" prefWidth="120.0" text="T.Mínima (Cº)" />
         <TableColumn fx:id="tempmax" prefWidth="120.0" text="T.Máxima (Cº)" />
         <TableColumn fx:id="datetime" prefWidth="120.0" text="Data" />
         <TableColumn fx:id="description" prefWidth="600.0" text="Descrição" />
     </columns>
   </TableView>
    <Text fx:id="instruction">
        <font>
            <Font name="Arial" size="12.0" />
        </font>
    </Text>
    <TextField fx:id="searchbox" prefHeight="34.0" prefWidth="100.0" />
    <Button fx:id="botao" mnemonicParsing="false" onAction="#handleButtonClick" text="Button" />
</VBox>
